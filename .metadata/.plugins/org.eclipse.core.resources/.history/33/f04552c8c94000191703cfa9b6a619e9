package org.hotelooka.service.booking;

import java.util.List;

import org.hotelooka.domain.HotelBooking;
import org.springframework.http.ResponseEntity;


public interface HotelBookingService {
	
	HotelBooking createBooking (HotelBooking hotelBooking);
	
	HotelBooking getBooking(Long id);
	
	ResponseEntity<?> deleteBooking(Long id);
	
	List<HotelBooking>  getAllBooking();
}
